{
  "name": "lightdash-client-typescript-fetch",
  "version": "0.0.3",
  "description": "TypeScript client for Lightdash API generated using openapi-fetch",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "generate": "npm run generate:client",
    "generate:client": "bash ./scripts/generate-client.sh",
    "lint:eslint": "eslint . --ext .ts",
    "lint:prettier": "prettier --check \"src/**/*.ts\"",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "fix:eslint": "eslint . --ext .ts --fix",
    "fix:prettier": "prettier --write \"src/**/*.ts\"",
    "fix": "npm run fix:prettier && npm run fix:eslint"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/syucream/lightdash-client-typescript-fetch.git"
  },
  "keywords": [
    "lightdash",
    "typescript",
    "openapi"
  ],
  "author": "syucream",
  "license": "MIT",
  "dependencies": {
    "openapi-fetch": "^0.9.1"
  },
  "devDependencies": {
    "typescript": "5.3.3",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "openapi-typescript": "^6.7.4",
    "prettier": "^3.1.1"
  }
}
